		</screen>
		<bezel element="bezel">
			<bounds x="0" y="0" width="4000" height="2250" />
			<color red="1.0" green="1.0" blue="1.0" alpha="1.0" />
		</bezel>
		<bezel element="screen">
			<bounds x="0" y="0" width="4000" height="2250" />
			<color red="1.0" green="1.0" blue="1.0" alpha="0.6" />
		</bezel>
		<bezel element="inst">
			<bounds x="2950" y="890" width="511" height="900" />
			<color red="1.0" green="1.0" blue="1.0" alpha="1.0" />
		</bezel>
	</view>	
	<view name="Instructions_Only">	
		<bezel element="inst">
			<bounds x="0" y="0" width="1135" height="2000" />
		</bezel>
	</view>
   </mamelayout>

